@Scripts.Render("~/bundles/enums")
@Scripts.Render("~/bundles/angularConsultant")
@{
    ViewBag.Title = "Edit";
}
<div ng-controller="consultantEditController">
    <div id="loadingDiv" ng-show="isloading">
        <span class="spinner"><i class="fa fa-spinner fa-spin fa-5x"></i></span>
    </div>

    <h2>Edit</h2>

    {
    "Id": 0,
    "Level": 0,
    "SkillsFile": "string",
    "ProjectId": 0,
    "ManagerId": 0,
    "User": {
    "Id": 0,
    "FirstName": "string",
    "LastName": "string",
    "Username": "string",
    "Email": "string",
    "Password": "string",
    "Salary": 0,
    "EntryDate": "2020-10-19T10:46:07.419Z"
    }
    <form novalidate>
        <div class="form-group">
            <label>Niveau</label>
            <select ng-options="item.id as item.label for item in ELevels" ng-model="level"></select>
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
        </div>
        <div class="form-check">
            <input type="checkbox" class="form-check-input" id="exampleCheck1">
            <label class="form-check-label" for="exampleCheck1">Check me out</label>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>


</div>